{
  "name": "quick-translate",
  "version": "0.1.0",
  "sources": {
    "dir": "src",
    "subdirs": true
  },
  "package-specs": [
    {
      "module": "esmodule",
      "in-source": true
    }
  ],
  "suffix": ".bs.mjs",
  "bs-dependencies": [
    "@rescript/core",
    "@rescript/react"
  ],
  "bsc-flags": ["-open RescriptCore"],
  "warnings": {
    "error": "+101"
  },
  "jsx": {
    "version": 4,
    "mode": "automatic"
  }
}
